/* QListWidget */

QListWidget {
	background: transparent;
	border: none;
}

QListWidget::item {
	border: none;
	color: #ffffff;
	padding: 8px 14px;
}

QListWidget::item:hover {
    background-color: #334449;
}

QListWidget::item:selected {
    background-color: #27a2bf;
}

QSplitter {
	background: #12191b;
}

QSplitter::handle {
	background: #12191b;
    border: none;
}

QToolTip {
	background-color: #243135;
	border: 1px solid #5f6f7a;
}

QTreeWidget {
	background: transparent;
	border: none;
	padding: 5px 10px;
}

QTreeWidget::item {
	border: none;
	outline: 0;
	color: white;
}

QMessageBox QPushButton {
	background-color: #334449;
	border: none;
	color: #ffffff;
	padding: 7px 12px;
}

QMessageBox QPushButton:hover,
QMessageBox QPushButton:pressed {
	background-color: #334449;
	border: 1px solid #00c2d3;
	color: #00c2d3;
}

QDialogButtonBox {
	qproperty-centerButtons: true;
}

QMenu {
	background: #243135;
	border: 1px solid #5f6f7a;
	padding: 8px 0;
}

QMenu::item {
    background-color: transparent;
	color: #5b8b97;	
}

QMenu QCheckBox:hover,
QMenu QRadioButton:hover {
	background: #334449;
	color: #ffffff;
}

QMenu::item:selected {
	background: #334449;
	color: #ffffff;
}

QMenu::separator {
    background: #5f6f7a;
    height: 1px;	
}

QMenu::indicator{	
}

QMenu::right-arrow{	
}

/* custom widgets */
QWidget#table-widgetitem,
QWidget#table-widgetitem QLabel {
	background: transparent;
	font-size: 10px;
}